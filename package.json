{
  "private": false,
  "name": "vuepress-plugin-posts-encrypt",
  "version": "0.0.2",
  "main": "lib/index.js",
  "repository": "https://github.com/alphawq/vuepress-plugin-posts-encrypt.git",
  "bugs": {
    "url": "https://github.com/alphawq/vuepress-plugin-posts-encrypt/issues",
    "email": "alphawq@foxmail.com"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "keywords": [
    "vue",
    "plugin",
    "encrypt",
    "vuepress",
    "vuepress-plugin",
    "vuepress-plugin-posts-encrypt"
  ],
  "author": "Aphasia <alphawq@foxmail.com> (https://github.com/alphawq)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@types/node": "~16.7.13",
    "clean-css": "^5.1.5",
    "crypto-js": "4.1.1",
    "less": "^4.1.1",
    "uglify-js": "^3.14.2",
    "vuepress": "^1.8.2",
    "vuepress-types": "^0.9.4"
  },
  "engines": {
    "node": ">=10"
  }
}
